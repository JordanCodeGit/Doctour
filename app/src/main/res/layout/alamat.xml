<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="383dp"
        android:layout_height="755dp"
        android:layout_marginBottom="84dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/img_5" />

    <EditText
        android:id="@+id/editTextTextPersonName9"
        android:layout_width="367dp"
        android:layout_height="177dp"
        android:layout_alignParentTop="true"
        android:background="@drawable/white_rectangle"
        android:gravity="top"
        android:hint="Isi alamatmu!"
        android:textAppearance="@style/enterPasswordTxt"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/kirim"
        app:layout_constraintEnd_toEndOf="@+id/imageView30"
        app:layout_constraintHorizontal_bias="0.375"
        app:layout_constraintStart_toStartOf="@+id/imageView30"
        app:layout_constraintTop_toTopOf="@+id/imageView30"
        app:layout_constraintVertical_bias="0.957" />

    <Button
        android:id="@+id/kirim"
        android:layout_width="129dp"
        android:layout_height="53dp"
        android:backgroundTint="#86A3F6"
        android:text="Kirim"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.796" />
</androidx.constraintlayout.widget.ConstraintLayout>